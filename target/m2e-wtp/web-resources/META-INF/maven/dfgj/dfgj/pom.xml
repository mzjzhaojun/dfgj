	<modelVersion>4.0.0</modelVersion>
	<groupId>dfgj</groupId>
	<artifactId>dfgj</artifactId>
	<version>1.1.1-SNAPSHOT</version>
	<name>dfgj</name>
	<packaging>war</packaging>

	<parent>
