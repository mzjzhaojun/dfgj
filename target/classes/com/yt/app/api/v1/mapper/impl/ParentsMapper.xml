  <!-- 默认新增 -->
  <insert id="post" parameterType="com.yt.app.api.v1.entity.Parents">
  insert into parents (id,parentid,parentcode,parentname,gender,email,industry,career,income,birthday,idtype,idnumber,country,province,city,county,streetname,addressdetail,creatorid,creatorname,createtime,modifierid,modifiername,modifytime,tenantcode,versionstarttime,versionendtime)
         values (#{id},#{parentid},#{parentcode},#{parentname},#{gender},#{email},#{industry},#{career},#{income},#{birthday},#{idtype},#{idnumber},#{country},#{province},#{city},#{county},#{streetname},#{addressdetail},#{creatorid},#{creatorname},#{createtime},#{modifierid},#{modifiername},#{modifytime},#{tenantcode},#{versionstarttime},#{versionendtime})
  </insert>
  <!-- 默认删除id对象 -->
 <delete id="delete" parameterType="java.lang.String">
     delete from parents 
     where id = #{id}
 </delete>
  <!-- 默认更新id对象 -->
 <update id="put" parameterType="com.yt.app.api.v1.entity.Parents">
    update parents
         <set>
             <if test="version != null">
                   version = #{version}+1,
             </if>
             <if test="parentid != null">
                   parentid = #{parentid},
             </if>
             <if test="parentcode != null">
                   parentcode = #{parentcode},
             </if>
             <if test="parentname != null">
                   parentname = #{parentname},
             </if>
             <if test="gender != null">
                   gender = #{gender},
             </if>
